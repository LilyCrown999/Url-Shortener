

{{#if uri}}
    <div class="panel panel-default well well-lg">
          <div class="panel-heading"><h4> Url's Shortener List of registered urls</h4></div>
   <table class=" table ">
       <thead class="">
           <tr>
               <th>Name</th>    
               <th>Alias</th>
               <th>Url</th>
               <th>Created at </th>
               <th>Updated at </th>
           </tr>
       </thead>
       <tbody>
           {{#each uri}}
           <tr>

               <td><span><a href="/{{alias}}" class="text-green">{{name}}</a></span></td>
               <td><span>{{alias}}</span></td>
               <td><span>{{url}}</span></td>
               <td><span>{{formatDate createdAt 'MMMM Do YYYY, h:mm:ss a'}}</span></td>
               <td><span>{{formatDate updatedAt 'MMMM Do YYYY, h:mm:ss a'}}</span></td>

            </tr>
    </div>
         {{/each}}

{{else}}
   <p> No registered urls yet</p>
{{/if}}
